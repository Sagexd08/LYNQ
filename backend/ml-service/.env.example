# ==============================================
# LYNQ ML Service Environment Configuration
# ==============================================

# Model Configuration
MODEL_SOURCE=local
# MODEL_SOURCE=s3

# S3 Configuration (if MODEL_SOURCE=s3)
S3_BUCKET=lynq-ml-models
S3_KEY=models/credit-risk-v1.pkl
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# API Security
API_KEY=your-ml-service-api-key

# SHAP Explainability (disable for faster inference)
ENABLE_SHAP=true

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Logging
LOG_LEVEL=INFO
